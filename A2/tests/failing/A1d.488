    % a program using non-recursive functions and procedures with and without parameters.
    % AUTHOR: g0getter.

    func printfunction():boolean{
        put "hello world"
        result true
    }

    func return_is_equal(a:integer,b:integer):boolean{
        var res:boolean
        res:= a = b
        result res
    }

    proc printproc(){
        put "hello world"
        return
    }

    proc print_is_equal(a:integer,b:integer){
        if a = b then
            put "equal"
        else
            put "not equal"
        fi
        return
    }

}