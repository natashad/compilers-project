    % test to pass in bad argument list to a procedure

    proc print_is_equal(a:integer,b:integer){
        if a = b then
            put "equal"
        else
            put "not equal"
        fi
        return
    }


    % Although the argument types don't match with the declaration, this
    % should not fail the parse.
    print_is_equal(true, true)

    % this shouldn't work since arguments don't take a type.
    print_is_equal(var a:integer,var b:integer)


}