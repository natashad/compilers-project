func printfunction():boolean{
    put "hello world"
    result true
}

func return_is_equal(var a:integer,var b:integer):boolean{
    var res:boolean
    res:= a = b
    result res
}

proc printproc(){
    put "hello world"
    return
}

proc print_is_equal(var a:integer,var b:integer){
    if a = b then 
        put "equal" 
    else 
        put "not equal"
}