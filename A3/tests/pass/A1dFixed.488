    % DECLARATIONS $

    func printfunction():boolean{
        put "hello world"
        result true
    }

    func return_is_equal(a:integer,b:integer):boolean {
        var res:boolean
        res:= a = b
        result res
    }

    proc printproc() {
        put "hello world"
        return
    }

    proc print_is_equal(a:integer,b:integer) {
        if a = b then
            put "equal"
        else
            put "not equal"
        fi
        return
    }

    var a:integer
    var b:integer

    % STATEMENTS %

    if printfunction() then
        put "output works"
    fi

    a:= 1
    b:= 2

    print_is_equal(a, b)
    print_is_equal(a+3, b-2)

}